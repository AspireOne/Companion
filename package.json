{
  "name": "companion",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build && prisma migrate deploy",
    "build:local": "next build",
    "build:native": "cross-env NEXT_PUBLIC_BUILDING_NATIVE=1 next build && next export",
    "dev": "next dev",
    "dev:external": "cross-env HOST=0.0.0.0 next dev",
    "start": "next start",
    "lint": "prettier . --write",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "extract": "lingui extract",
    "compile": "lingui compile"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lingui extract $(git diff --name-only --staged) --clean"
    }
  },
