{
  "name": "companion",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:prod": "next build && prisma migrate deploy",
    "build:native": "cross-env NEXT_PUBLIC_BUILDING_NATIVE=1 next build",
    "dev": "cross-env NEXT_PUBLIC_DEVELOPMENT=1 next dev",
    "dev:external": "cross-env HOST=0.0.0.0 next dev",
    "start": "next start",
    "lint": "prettier . --write",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "t:extract": "lingui extract",
    "t:compile": "lingui compile",
    "t:push": "crowdin push sources && crowdin push translations",
    "t:push:translate": "crowdin push sources && crowdin push translations && pnpm t:pre-translate",
    "t:pull": "crowdin pull && pnpm t:compile",
    "t:pre-translate": "crowdin pre-translate --method=mt --engine-id=410300",
    "t:sync": "pnpm t:extract && pnpm t:push && pnpm t:pull",
    "t:sync:translate": "pnpm t:extract && pnpm t:push && pnpm t:pre-translate && pnpm t:pull",
    "prisma:reset": "prisma db push --force-reset && prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lingui extract $(git diff --name-only --staged) --clean"
    }
  },
